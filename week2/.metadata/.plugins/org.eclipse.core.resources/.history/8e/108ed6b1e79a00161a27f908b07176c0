#include <iostream>
using namespace std;

int main() {
	std::ios_base::sync_with_stdio(false)
	int test_case;
	for (int test_num = 0; test_num < test_case; test_num ++){
		int num1,num2;
		cin >> num1 >> num2;
	}
	return 0;
}
