#include <iostream>
#include <cassert>
#include <CGAL/basic.h>
#include <CGAL/QP_models.h>
#include <CGAL/QP_functions.h>
#include <CGAL/Gmpz.h>

using namespace std;


int main() {
	int p ;
	while (cin >> p && p != 0){
		int p,a,b;
		cin >> p >> a >> b;
		if (p == 1){
			do_case1(a,b);
		}
		else do_case2(a,b);
	}
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
