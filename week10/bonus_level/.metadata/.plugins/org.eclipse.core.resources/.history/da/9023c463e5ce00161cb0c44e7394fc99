// The forward way is a easy DP, what is the return?
//
#include <iostream>
#include <vector>
using namespace std;
typedef vector<vector<int> > mat;
void do_case(){
	int n;
	cin >> n;
	mat original(n,vector<int>(n,0));
	for (int i = 0; i < n ; i ++){
		for (int j = 0; j< n; j++){
			cin >> original[i][j]
		}
	}
}

int main() {

	int t;
	cin >> t;
	while(t--){
		do_case();
	}

	return 0;
}
